{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",          // onde está seu código TS
    "outDir": "dist",          // onde os .js compilados vão parar
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}
